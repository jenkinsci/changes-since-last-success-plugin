<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
      <groupId>org.jenkins-ci.plugins</groupId>
      <artifactId>plugin</artifactId>
      <version>1.424</version>
    </parent>

    <artifactId>changes-since-last-success</artifactId>
    <packaging>hpi</packaging>
    <name>Changes since last successful build Plugin</name>
    <version>0.7-SNAPSHOT</version>
    <url>http://wiki.jenkins-ci.org/display/JENKINS/Changes+Since+Last+Success+Plugin</url>
    <description>generate changelog for a range of builds</description>

	<properties>
	    <maven-release-plugin.version>2.5.3</maven-release-plugin.version>
	</properties>
	
    <developers>
      <developer>
        <id>ndeloof</id>
        <name>Nicolas De Loof</name>
      </developer>
    </developers>

    <licenses>
      <license>
        <name>LGPL</name>
      </license>
    </licenses>

    <scm>
      <connection>scm:git:git://github.com/jenkinsci/changes-since-last-success-plugin.git</connection>
      <developerConnection>scm:git:git@github.com:jenkinsci/changes-since-last-success-plugin.git</developerConnection>
      <url>https://github.com/jenkinsci/changes-since-last-success-plugin</url>
      <tag>HEAD</tag>
  </scm>

    <distributionManagement>
      <repository>
        <id>maven.jenkins-ci.org</id>
        <url>https://repo.jenkins-ci.org/releases/</url>
      </repository>
      <snapshotRepository>
        <id>maven.jenkins-ci.org</id>
        <url>https://repo.jenkins-ci.org/snapshots/</url>
      </snapshotRepository>
    </distributionManagement>
 
    <repositories>
        <repository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </pluginRepository>
    </pluginRepositories>
</project>  
  

